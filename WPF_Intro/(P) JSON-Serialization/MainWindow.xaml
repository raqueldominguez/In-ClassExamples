<Window x:Class="_P__JSON_Serialization.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_P__JSON_Serialization"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="cboFilter" HorizontalAlignment="Left" Margin="84,63,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cboFilter_SelectionChanged"/>
        <ListBox x:Name="lstGames" Margin="400,0,0,0" MouseDoubleClick="lstGames_MouseDoubleClick_1"/>
        <Label x:Name="lblResultCount" Content="Result Count: 1,800" HorizontalAlignment="Left" Margin="259,358,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnExport" Content="Button" HorizontalAlignment="Left" Margin="59,311,0,0" VerticalAlignment="Top" Click="btnExport_Click"/>

    </Grid>
</Window>
